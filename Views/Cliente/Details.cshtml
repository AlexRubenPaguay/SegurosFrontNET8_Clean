@model SegurosFrontNET8_Clean.Models.Cliente

@{
    ViewData["Title"] = "Details";
}

<h3>Detalles de Cliente: @Html.DisplayFor(model => model.Nombre)</h3>

<div>    
    
    <dl class="row">        
        <dt class = "col-sm-2">
            Cédula
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Cedula)
        </dd>        
        <dt class = "col-sm-2">
            Teléfono
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Telefono)
        </dd>
        <dt class = "col-sm-2">
            Edad
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Edad)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Edit", "Edit", new {  id = Model.Cedula }) |
    <a asp-action="Index">Retornar</a>
</div>
